<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping schema="SSS_DICTIONARY_DB" package="tn.com.smartsoft.iap.dictionary.graphique.actionTemplate.beans">
	<class name="ActionTemplateBean" table="UI_ACTION_TEMPLATES">
		<id name="actionTemplateId" column="UI_ACTION_TEMPLATE_ID">
