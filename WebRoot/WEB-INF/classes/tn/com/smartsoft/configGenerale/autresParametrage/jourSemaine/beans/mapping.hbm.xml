<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping schema="sss_config_general_db" package="tn.com.smartsoft.configGenerale.autresParametrage.jourSemaine.beans">
	<class name="JourSemaineBean" table="cr_jour_semaines">
		<meta attribute="entite-id">typePersonnel</meta>
		<id name="jourId" column="cr_jour_id">
			<generator class="assigned" />
		</id>
		<property name="libelle" column="libelle" />
		<property name="abreviation" column="abreviation" />
		<property name="jourCode" column="jour_code" />
		<property name="rang" column="rang" />
	</class>
</hibernate-mapping>