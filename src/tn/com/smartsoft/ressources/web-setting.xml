<?xml version="1.0" encoding="UTF-8"?>
<web-setting xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://tn.com.smartsoft/dtds/web-setting.xsd">
	<encoding value="UTF-8" />
	<locale value="FR" />
	<window-response-view value="window" />
	<tags location="tn/com/smartsoft/ressources/xml-tags.xml" />
	<web-beans location="tn/com/smartsoft/ressources/formater-setting.xml" />
	<scripts location="tn/com/smartsoft/ressources/scripts-setting.xml" />
	<images location="tn/com/smartsoft/ressources/web-image-path.xml" />
	<response-views>
		<response-view name="file" class="tn.com.smartsoft.framework.presentation.view.response.ResponseFile" content-type="text/file">
			<header name="Cache-Control" value="max-age=0, must-revalidate, no-cache, no-store, private, post-check=0, pre-check=0" />
			<header name="Pragma" value="no-cache" />
			<header name="Expires" value="0" />
		</response-view>
		<response-view name="window" class="tn.com.smartsoft.framework.presentation.view.response.WindowResponse" content-type="text/html">
